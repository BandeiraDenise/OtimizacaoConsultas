{
  "name": "oracle-db",
  "dockerFile": "Dockerfile",
  "context": ".",
  "service": "oracle",
  "workspaceFolder": "/workspaces",
  "extensions": [
    "oracle.oracle-developer-tools-for-visual-studio-code"
  ],
  "settings": {
    "sqltools.connections": [
      {
        "name": "OracleDB",
        "driver": "Oracle",
        "server": "localhost",
        "port": 1521,
        "database": "ORCL",
        "username": "system",
        "password": "oracle",
        "connectString": "localhost:1521/ORCL"
      }
    ]
  },
"postCreateCommand": "bash ./OtimizacaoConsultas/dados/init.sh"
}
