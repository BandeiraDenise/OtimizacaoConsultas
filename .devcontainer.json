{
    "name": "Oracle DB with SQL*Plus",
    "dockerComposeFile": "docker-compose.yml",
    "service": "oracle",
    "workspaceFolder": "/workspaces/otimizacaoconsultas",
    "extensions": [
      "ms-azuretools.vscode-docker"
    ],
    "settings": {
      "terminal.integrated.shell.linux": "/bin/bash"
    },
    "postCreateCommand": "docker-compose up -d && sleep 60 && docker exec -it oracle-db /bin/bash"
  }
  
